<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Ejemplo_Flyout.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Ejemplo_Flyout"
    xmlns:pages="clr-namespace:Ejemplo_Flyout.Pages"    
    Title="Ejemplo_Flyout">

    <!--<ShellContent
    Title="Home"
    ContentTemplate="{DataTemplate local:MainPage}"
    Route="MainPage" />-->

    <FlyoutItem Title="Home">
        <!--Icon="cat.png">-->
        <FlyoutItem.Icon>
            <FontImageSource Glyph="home" FontFamily="MaterialIconsOutlined"  Size="24" Color="Black"  />
        </FlyoutItem.Icon>

        <Tab>
            <ShellContent ContentTemplate="{DataTemplate pages:APage}" />
        </Tab>
    </FlyoutItem>
    <FlyoutItem Title="Cats">
        <!--Icon="cat.png">-->
        <FlyoutItem.Icon>
            <FontImageSource Glyph="contact_page" FontFamily="MaterialIconsOutlined"  Size="24" Color="Black"  />
        </FlyoutItem.Icon>

        <Tab>
            <ShellContent ContentTemplate="{DataTemplate pages:APage}" />
        </Tab>
    </FlyoutItem>
    <FlyoutItem Title="Configuracion">
        <FlyoutItem.Icon>
            <FontImageSource Glyph="settings" FontFamily="MaterialIconsOutlined"  Size="24" Color="Black"  />
        </FlyoutItem.Icon>

        <Tab>
            <ShellContent ContentTemplate="{DataTemplate pages:BPage}" />
        </Tab>
    </FlyoutItem>
</Shell>
